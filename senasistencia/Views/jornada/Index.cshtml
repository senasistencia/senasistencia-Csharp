@model IEnumerable<senasistencia.Models.jornada>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="highlight bordered responsive-table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Descripcion_Jornada)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Estado_Jornada)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FechaDeCreacion_Jornada)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FechaDeInactivacion_Jornada)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Descripcion_Jornada)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Estado_Jornada)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FechaDeCreacion_Jornada)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FechaDeInactivacion_Jornada)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ID_Jornada }) |
            @Html.ActionLink("Details", "Details", new { id=item.ID_Jornada }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ID_Jornada })
        </td>
    </tr>
}

</table>
